{
    "manifest_version": 2,
    "minimum_chrome_version": "6.0",
    "author": "shadowbq",
    "content_scripts": [{
        "css": ["css/compact.css"],
        "matches": ["https://outlook.live.com/*", "https://outlook.live.com/*", "https://outlook.office.com/*", "http://outlook.office.com/*", "https://outlook.office365.com/*", "http://outlook.office365.com/*"],
        "run_at": "document_start"
    }],
    "options_page": "options.html",
    "description": "Style Script for Outlook Office365 to create a compact view",
    "name": "Office365 Outlook Compact View",
    "update_url": "https://clients2.google.com/service/update2/crx",
    "homepage_url": "https://chrome.google.com/webstore/detail/npdfmcdicjcimlknokcglcobjbckkmkh",
    "short_name": "O365 CV",
    "version": "1.0.13",
    "icons": {
        "128": "icon/128.png",
        "16": "icon/16.png",
        "32": "icon/32.png",
        "64": "icon/64.png"
    }
}
